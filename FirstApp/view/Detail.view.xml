<mvc:View
	controllerName="akp.po.walkthrough.controller.Detail"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		title="{i18n>detailPageTitle}"
		showNavButton="true"
		navButtonPress=".onNavBack">
		<ObjectHeader
			intro="{pos>Ebeln}"
			title="{pos>Matnr}"/>

	<List
	
		items="{
			path: 'pos>items'
		}" >
		<StandardListItem
			title="{pos>item}"/>
	</List>			
	</Page>
</mvc:View>