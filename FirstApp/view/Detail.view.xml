<mvc:View
	controllerName="akp.po.walkthrough.controller.Detail"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page id="idDetailParent"
		title="{i18n>detailPageTitle}"
		showNavButton="true"
		navButtonPress="onBack">
		<ObjectHeader
			intro="{pos>Ebeln}"
			title="{pos>Matnr}"/>

	<List
	
		items="{
			path: 'pos>items'
		}" >
		<StandardListItem
			title="{pos>item}"/>
	</List>	

	<content>
		<!-- two views will be inserted here via routing  -->
	</content>


	</Page>
</mvc:View>